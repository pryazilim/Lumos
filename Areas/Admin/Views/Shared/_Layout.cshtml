<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <meta name="robots" content="noindex">
  <meta name="robots" content="nofollow">
  <meta name="description" content="Pr yazilim Admin">
  <title>@ViewBag.Title | Lumos </title>
  <link rel="icon" href="~/Content/Admin/images/favicon.ico" type="image/x-icon">
  <link rel="stylesheet" href="~/css/admin/nice-select.css">

  <link rel="stylesheet" href="~/css/admin/style.css">

  <script src="~/lib/jquery/dist/jquery.js" asp-append-version="true"></script>
  <script src="~/lib/ckeditor/ckeditor.js" asp-append-version="true"></script>
</head>
<body>
  <div class="overlaypage"></div>
  <div class="overlaypage1"></div>
  <header class="w-100 flex text-center h-80 border-bottom-1 justify-between align-center bg-white px-24">
    <div class="min-width-d-none">
      <div class="open-left-bar cursor-pointer">
        <img class="w-24 h-24 openbutton" src="/img-content/admin/icon/Menu.svg" alt="menuicon" />
        <img class="w-24 h-24  closebutton" src="/img-content/admin/icon/Close-blue-icon.svg" alt="close">
      </div>
      <div class="sidebar-left overflow-y ">
        <ul class="">
          <li><a href="/admin">Anasayfa</a></li>
          <li><a class="flex justify-between " href="#">Sabit İçerikler<img class="w-16 h-16" src="/img-content/admin/icon/Plus-icon.svg" alt="plus" /></a></li>
          @*<li><a href="/admin/CategoryManagement">Kategoriler</a></li>*@
          <li><a href="/admin/ServicesList">Hakkımızda</a></li>
          <li><a href="/admin/AboutList">Hizmetler</a></li>
          <li><a href="/admin/ProductList">Ürünler</a></li>
          <li><a href="/admin/AboutList">Hizmetler</a></li>
          <li><a href="/admin/ProjectsList">Projeleri</a></li>
          <li><a href="/admin/BlogAndNewsList">Haberler/Bloglar</a></li>
          <li><a href="/admin/ReferenceList">Referanslar</a></li>
          <li><a href="/admin/PagesList">Sayfalar</a></li>
          <li><a href="/admin/ContractsList">Sözleşmeler</a></li>
          @*<li><a href="/admin/GalleryList">Galeri</a></li>*@
          <li><a href="/admin/Contact">Form Yönetimi</a></li>
          @*<li><a href="#">S.S.S Yönetimi</a></li>*@
        </ul>
      </div>
    </div>
    <div class="">
      <a href="/admin/" class="cursor-pointer">
        <img class=" w-135 h-40" src="~/Content/Site/img/icon/logo-2.svg" alt="logo" />
      </a>
    </div>
    <div class="min-width-d-none">
      <div class="open-right-bar cursor-pointer">
        <img class="w-24 h-24 useropen" src="/img-content/admin/icon/User-circle.svg" alt="usericon" />
        <img class="w-24 h-24 userclose" src="/img-content/admin/icon/Close-blue-icon.svg" alt="closeuser">
      </div>
      <div class="sidebar-right">
        <ul class="">
          <li><a class="flex justify-between text-blue" href="#">Siteye Git <img class="w-16 h-16" src="/img-content/admin/icon/home-blue-icon.svg" alt="home" /></a></li>
          <li><a class="flex justify-between" href="#">Kullanıcı Ayarları<img class="w-16 h-16" src="/img-content/admin/icon/Settings.svg" alt="Settings" /></a></li>
          <li><a class="flex justify-between" href="/admin/logout">Çıkış Yap<img class="w-16 h-16" src="/img-content/admin/icon/Logout.svg" alt="Logout" /></a></li>
        </ul>
      </div>
    </div>
    <nav class="display-none-M">
      <ul class="flex m-0 text-font-500 text-12 text-secondary headerNavbarLinksText h-100">
        <li class="px-16 "><a class="" href="/admin">Anasayfa</a></li>
        <li class="px-16 openSabitDropDown cursor-pointer text-primary">
          Sabit İçerikler
          <div class="sabitDropDown absolute right-0 mr-856 px-16 py-16 bg-white h-auto box-shadow-primary rounded w-208 grid gap-x-2 mt-30">
            <a class="flex justify-between" href="/admin/slider-management">Ana Slider Yönetimi</a>
            <a class="flex justify-between" href="/admin/IndexContentList">Ana Sayfa İçerik Yönetimi</a>
            <a class="flex justify-between" href="/admin/OtherFixedPageList">Diğer Sabit Sayfa</a>
            <a class="flex justify-between text-left" href="/admin/ContractsList">Politika veya Sözleşme Yönetimi</a>
            <a class="flex justify-between" href="/admin/FAQList">Destek/SSS</a>
          </div>
        </li>
        @*<li class="px-16"><a href="/admin/CategoryManagement">Kategoriler</a></li>*@
        <li class="px-16"><a href="/admin/ServicesList">Hakkımızda</a></li>
        <li class="px-16"><a href="/admin/AboutList">Hizmetler</a></li>
        <li class="px-16 openSabitDropDown cursor-pointer text-primary">
          Ürünler
          <div class="sabitDropDown absolute right-0 mr-special px-16 py-16 bg-white h-auto box-shadow-primary rounded w-208 grid gap-x-2 mt-30">
            <a class="flex justify-between" href="/admin/ProductList">Ürün Listesi</a>
            @*<a class="flex justify-between" href="/admin/ProductPropertyList">Ürün Özellikleri</a>*@
          </div>
        </li>
        @*<li class="px-16"><a href="/admin/ProductList">Ürünler</a></li>
          <li class="px-16"><a href="/admin/ProductPropertyList">Ürün Özellikleri</a></li>*@
        <li class="px-16"><a href="/admin/BlogAndNewsList">Haberler/Bloglar</a></li>
        <li class="px-16"><a href="/admin/ReferenceList">Referanslar</a></li>
        <li class="px-16"><a href="/admin/PagesList">Sayfalar</a></li>
        @*<li class="px-16"><a href="/admin/ContractsList">Sözleşmeler</a></li>*@
        @*<li class="px-16"><a href="/admin/GalleryList">Galeri</a></li>*@
        <li class="px-16"><a href="/admin/Contact">Form Yönetimi</a></li>
        @*<li class="px-16"><a href="#">S.S.S Yönetimi</a></li>*@
        <li class="px-16"><img class="h-16 w-0" src="/img-content/admin/icon/header-line.svg" alt="line" /></li>
        <li class="px-16">
          <img class="w-24 h-24 -mt-6 cursor-pointer openUserDropDown" src="/img-content/admin/icon/User-circle.svg" alt="user" />
          <div class="userDropDown absolute right-0 mr-39 px-16 py-16 bg-white h-auto box-shadow-primary rounded w-208 grid gap-y-2 mt-30">
            <a target="_blank" class="flex justify-between text-blue linkGoWeb" href="/">Siteye Git <img class="w-16 h-16" src="/img-content/admin/icon/home-blue-icon.svg" alt="home" /></a>
            <a class="flex justify-between" href="#">Kullanıcı Ayarları<img class="w-16 h-16" src="/img-content/admin/icon/Settings.svg" alt="Settings" /></a>
            <a class="flex justify-between" href="/admin/logout">Çıkış Yap<img class="w-16 h-16" src="/img-content/admin/icon/Logout.svg" alt="Logout" /></a>
          </div>
        </li>
      </ul>
    </nav>
  </header>

  <section class="content">
    @RenderBody()
  </section>

  <div class="h-55 bottom-0 fixed w-100 flex justify-center justify-between-M align-center bg-white py-16 px-16">
    <a class="text-font-400 text-blue text-10 ml-auto m-none-M" href="#"><span class="text-third">©2022 - Pr Yazılım</span> Uniq CMS</a>
    <div class="ml-auto flex m-none-M">
      <a href="#" class="ml-auto text-blue text-10 text-font-500"><img class="w-12 h-12 mr-6" src="/img-content/admin/icon/footer-phone-icon.svg" alt="phone" />+90 232 700 0770</a>
    </div>
  </div>
  <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
  <script src="~/js/Admin/jquery.nice-select.js" asp-append-version="true"></script>

  <script src="~/js/Admin/script.js" asp-append-version="true"></script>
  <script>
    var pathname = window.location.pathname;
    $('li.active').not('.TabMenu').removeClass('active');
    $('a[href="' + pathname + '"]').closest("li").addClass("active");
  </script>
</body>
</html>

